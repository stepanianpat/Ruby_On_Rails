<h1>Listing <%= @count %> Dojos</h3>
<table>
  <tr>
    <th>Coding Dojo Branch</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
    <th>Actions</th>
  </tr>

  <% @dojo.each do |dojo| %>
  <tr>
    <td><%= dojo.branch %></td>
    <td><%= dojo.street %></td>
    <td><%= dojo.city %></td>
    <td><%= dojo.state %></td>
    <td><a href="#">Show</a> | <a href="#">Edit</a> | <a href="#">Destroy</a></td>
  </tr>
  <% end %>
</table>

<h2><a href="/dojos/new">New Dojo</a></h2>
