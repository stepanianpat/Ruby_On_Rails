<h1> <%= @dojo.branch %>'s <%= @student.first_name %> <%= @student.last_name %></h1>

<h3>Email: <%= @student.email %></h3>

<h2>Cohort</h2>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
  </tr>

  <% @cohort.each do |student| %>
  <tr>
    <td><%= student.first_name %></td>
    <td><%= student.last_name %></td>
    <td><%= student.email %></td>
  </tr>
  <% end %>
</table>

<h4><a href="/goback">Home</a> | <a href="/dojos/<%= @dojo.id %>/students/<%= @student.id %>/edit">Edit</a> </h4>
